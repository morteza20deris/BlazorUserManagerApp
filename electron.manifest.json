{
  "executable": "BlazorUserManagerApp",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "BlazorUserManagerApp",
  "author": "",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "com.BlazorUserManagerApp.app",
    "productName": "BlazorUserManagerApp",
    "copyright": "Copyright Â© 2020",
    "win": {
      "target": [ "nsis" ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    },
    "buildVersion": "0.2.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}